<view >
<image mode="widthFix" class="weight-per100 height-per100" src="{{url}}"/>
</view>

<poster class="poster" id="poster" config="{{posterConfig}}" bind:success="onPosterSuccess"  bind:fail="onPosterFail">
</poster>

<view class="album" wx:if="{{url}}">
  <van-button type="danger" size="large" class="album__btn" bindtap="onSaveToAlbum">保存到相册</van-button>
</view>

<van-dialog id="van-dialog" />
<van-toast id="van-toast" />
